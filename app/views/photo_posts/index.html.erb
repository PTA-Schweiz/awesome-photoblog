<h1>Recent Posts</h1>
<div class="row">
  <% @photo_posts.each do |post| %>
    <div class="col-sm-4 col-md-3">
      <div class="thumbnail">
        <%= image_tag post.photo_url(:thumb) %>
        <div class="caption">
          <h3><%= post.name %></h3>
          <p>Photo by <%= post.user.email %></p>
        </div>
      </div>
    </div>
  <% end %>
</div>